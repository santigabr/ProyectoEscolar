---
import Header from "../components/header.astro"

interface Props {
  title: string
}
const { title } = Astro.props
---

<!doctype html>
<html lang="es">
  <head>
    <meta charset="UTF-8" />
    <meta
      name="description"
      content="Pagina para proyecto escolar de empresa remil picada"
    />
    <meta name="viewport" content="width=device-width" />
    <title>{title}</title>
  </head>
  <body>
    <Header />
    <main class="text-black">
      <slot />
    </main>

  </body>
</html>

<style is:global></style>
